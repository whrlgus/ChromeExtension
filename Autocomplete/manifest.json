{
    "name": "autocomplete",
    "description" : "귀찮게 입력하지 마러",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
      },
      "commands": {
        "_execute_browser_action": {
          "suggested_key": {
            "default": "Ctrl+Shift+Z",
            "mac": "MacCtrl+Shift+Z"
          },
          "description": "Opens popup.html"
        }
      },
      "permissions": ["tabs", "<all_urls>"]
  }
